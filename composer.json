{
    "name": "adtalemtools/adtalem-tools",
    "description": "Drush commands and shell scripts that help manage the Adtalem Drupal projects.",
    "type": "drupal-drush",
    "keywords": ["drush", "drupal", "adtalem", "scripts"],
    "authors": [
        {
            "name": "Adtalem Global Education",
            "homepage": "https://www.adtalem.com"
        }
    ],
    "require": {
        "php": "^7",
        "composer/installers": "^1.4",
        "drush/drush": "^9.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~5",
        "squizlabs/php_codesniffer": "~3"
    },
    "autoload-dev": {
        "psr-4": {
          "UsersCommands\\Tests\\": "tests/",
          "Unish\\": "vendor/drush/drush/tests/"
        }
    },
    "extra": {
        "drush": {
            "services": {
                "drush.services.yml": "^9.4"
            }
        },
        "installer-name": "UsersCommands"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
