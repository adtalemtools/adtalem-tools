{
  "name": "adtalemtools/adtalem-tools",
  "description": "Drush commands and shell script tools for managing Adtalem Drupal projects.",
  "type": "drupal-drush",
  "homepage": "https://github.com/adtalemtools/adtalem-tools",
  "authors": [
    {
      "name": "Adtalem Global Education",
      "homepage": "https://www.adtalem.com",
      "role": "Company"
    },
    {
      "name": "Robert Shell (robertshell22)",
      "homepage": "https://www.drupal.org/u/robertshell22",
      "role": "Maintainer"
    }
  ],
  "license": "GPL-2.0+",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7",
    "composer/installers": "^1.4",
    "drush/drush": "^9.4"
  },
  "require-dev": {
    "phpunit/phpunit": "~5",
    "squizlabs/php_codesniffer": "~3"
  },
  "autoload": {
    "psr-4": {
      "src\\deploy\\":  "deploy",
      "src\\users\\":  "users"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "adtalem_tools\\Tests\\": "tests/",
      "Unish\\": "vendor/drush/drush/tests/"
    }
  },
  "extra": {
    "installer-name": "adtalem_tools",
      "services": {
        "drush.services.yml": "^9.4"
      }
    }
  }
}
